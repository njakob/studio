{
  "name": "studio",
  "private": true,
  "version": "0.0.0",
  "description": "My studio tools",
  "author": "Nicolas Jakob <nicow.jakob@gmail.com>",
  "scripts": {
    "lint": "eslint --quiet --ext js,ts,tsx .",
    "type-check": "tsc --noEmit -p ./src/studio/application && tsc --noEmit -p ./src/studio/collector && tsc --noEmit -p ./src/studio/scripts",
    "test": "npm run lint && npm run type-check",
    "start": "node -r ./register.js ./node_modules/.bin/webpack-dev-server --config src/studio/scripts/webpack.dev.js",
    "build": "NODE_ENV=production node -r ./register.js ./node_modules/.bin/webpack --config src/studio/scripts/webpack.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "^5.0.1",
    "@types/webpack": "^4.41.10",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-const-enum": "1.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "styled-components": "^5.1.0",
    "styled-normalize": "^8.0.7",
    "terser-webpack-plugin": "^2.3.5",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
